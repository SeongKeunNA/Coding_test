   "source": [
    "import heapq\n",
    "n = int(input())\n",
    "q = []\n",
    "result = 0\n",
    "for _ in range(n):\n",
    "    q.append(int(input()))\n",
    "while len(list(q)) > 1:\n",
    "    a = heapq.heappop(q)\n",
    "    b = heapq.heappop(q)\n",
    "    result += a + b\n",
    "    heapq.heappush(q, a + b)\n",
    "print(result)"
   ]
  }
