    "def m(N):\n",
    "    c_500=N//500\n",
    "    N%=500\n",
    "    if N==0:\n",
    "        return c_500\n",
    "    c_100=N//100\n",
    "    N%=100\n",
    "    if N==0:\n",
    "        return c_500+c_100\n",
    "    c_50=N//50\n",
    "    N%=50\n",
    "    if N==0:\n",
    "        return c_500+c_100+c_50\n",
    "    c_10=N//10\n",
    "    return c_500+c_100+c_50+c_10\n",
