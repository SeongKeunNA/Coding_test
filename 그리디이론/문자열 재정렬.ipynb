    "S=input('문자열입력: ') #1<=len(S)<=10000\n",
    "s=[]\n",
    "n=[]\n",
    "for i in range(len(S)):\n",
    "    try:\n",
    "        n.append(int(S[i]))\n",
    "\n",
    "    except ValueError:\n",
    "        s.append(S[i])\n",
    "        \n",
    "    \n",
    "s=sorted(s)\n",
    "val=0\n",
    "for i in n:\n",
    "    val+=i\n",
    "s.append(val)\n",
    "print(*s,sep='')"
   ]
  }
