    "k=list(map(int,input().split()))\n",
    "f=[0]*100\n",
    "f[0]=k[0]\n",
    "f[1]=max(k[0],k[1])\n",
    "for i in range(2,n):\n",
    "    f[i]=max(f[i-1],f[i-2]+k[i])\n",
    "print(f[n-1])"
