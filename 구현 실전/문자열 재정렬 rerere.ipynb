    "data = list(input())\n",
    "value = 0\n",
    "array = []\n",
    "for d in data:\n",
    "    if d.isdigit():\n",
    "        value += int(d)\n",
    "        continue\n",
    "    array.append(d)\n",
    "array.sort()\n",
    "array.append(str(value))\n",
    "print(''.join(array))"
   ]
